{
  "buildOptions": {
    "additionalArguments": [ "/additionalfile:../../stylecop.json" ],
    "keyFile": "../../FluentRestBuilder.snk",
    "nowarn": [
      "CS1591"
    ],
    "warningsAsErrors": true,
    "xmlDoc": true
  },
  "dependencies": {
    "FluentRestBuilder": {
      "target": "project"
    },
    "Newtonsoft.Json": "9.0.1",
    "Microsoft.CodeAnalysis.FxCopAnalyzers": {
      "version": "1.2.0-beta2",
      "type": "build"
    },
    "StyleCop.Analyzers": {
      "version": "1.0.0",
      "type": "build"
    },
    "NETStandard.Library": "1.6.1"
  },
  "frameworks": {
    "net451": {},
    "netstandard1.6": {}
  },
  "packOptions": {
    "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
    "projectUrl": "https://github.com/kyubisation/FluentRestBuilder",
    "repository": {
      "type": "git",
      "url": "https://github.com/kyubisation/FluentRestBuilder"
    },
    "requireLicenseAcceptance": false
  },
  "version": "1.0.0-*"
}
